{
  "name": "boilerplate-backend",
  "version": "1.0.0",
  "description": "Basic backend boilerplate",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "dev": "babel-watch src/index.js",
    "start": "babel-node src/index.js",
    "test": "",
    "sequelize": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*",
    "db:create": "./node_modules/.bin/sequelize db:create",
    "db:drop": "./node_modules/.bin/sequelize db:drop",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migrate:reset": "./node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "author": "Verdotte Aututu",
  "license": "ISC",
  "devDependencies": {
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.8.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.12.13",
    "@hapi/joi": "^17.1.0",
    "babel-watch": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.9.1",
    "mysql2": "^2.2.5",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^6.2.0",
    "socket.io": "^2.3.0",
    "winston": "^3.2.1"
  }
}
